<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="action_sms_dashboard_landing" model="ir.actions.client">
            <field name="name">SMS Dashboard</field>
            <field name="tag">sms_dashboard_client_action</field> </record>

        <record id="view_sms_campaign_graph" model="ir.ui.view">
            <field name="name">sms.campaign.graph</field>
            <field name="model">sms.campaign</field>
            <field name="arch" type="xml">
                <graph string="SMS Usage" type="bar">
                    <field name="create_date" interval="month"/>
                    <field name="total_cost" type="measure"/>
                </graph>
            </field>
        </record>
    </data>
</odoo>